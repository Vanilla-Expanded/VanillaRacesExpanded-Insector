<?xml version="1.0" encoding="UTF-8"?>
<Defs>



  <ThingDef ParentName="SkyfallerBase">
    <defName>VRE_InfestedShipPart</defName>
    <label>infested ship part</label>
    <thingClass>DropPodIncoming</thingClass>
    <graphicData>
      <texPath>Structure/InfestedShipChunkDebris</texPath>
      <graphicClass>Graphic_Random</graphicClass>
      <shaderType>CutoutFlying</shaderType>
      <drawSize>1.9</drawSize>
    </graphicData>
    <skyfaller>
      <movementType>Decelerate</movementType>
      <shadow>Things/Skyfaller/SkyfallerShadowDropPod</shadow>
      <shadowSize>(2, 2)</shadowSize>
      <anticipationSound>DropPod_Fall</anticipationSound>
      <anticipationSoundTicks>100</anticipationSoundTicks>
      <motesPerCell>0</motesPerCell>
      <ticksToImpactRange>300~450</ticksToImpactRange>
      <angleCurve>
        <points>
          <li>(0,-60)</li>
          <li>(1,-90)</li>
        </points>
      </angleCurve>
      <rotationCurve>
        <points>
          <li>(0,30)</li>
          <li>(0.5,5)</li>
          <li>(0.9,-5)</li>
          <li>(0.95,0)</li>
        </points>
      </rotationCurve>
      <zPositionCurve>
        <points>
          <li>(0.95,0.25)</li>
          <li>(1,0)</li>
        </points>
      </zPositionCurve>
      <speedCurve>
        <points>
          <li>(0.6,0.6)</li>
          <li>(0.95,0.5)</li>
        </points>
      </speedCurve>
    </skyfaller>
  </ThingDef>

 
</Defs>