<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	
	
	<HediffDef>
		<defName>VRE_InsectJellyProduction</defName>
		<label>jelly production</label>
		<description>This person has large jelly sacks present on their lower abdomen, allowing them to periodically produce valuable insect jelly.</description>
		<hediffClass>HediffWithComps</hediffClass>
		<scenarioCanAdd>false</scenarioCanAdd>
		<maxSeverity>1.0</maxSeverity>
		<isBad>false</isBad>
		<comps>
			<li Class="VEF.AnimalBehaviours.HediffCompProperties_Spawner">
				<thingToSpawn>InsectJelly</thingToSpawn>
				<spawnCount>6</spawnCount>
				<spawnIntervalRange>60000~60000</spawnIntervalRange>
				<spawnMaxAdjacent>30</spawnMaxAdjacent>
				<spawnForbidden>false</spawnForbidden>
				<showMessageIfOwned>false</showMessageIfOwned>
				
			</li>
		</comps>
	</HediffDef>
	
</Defs>